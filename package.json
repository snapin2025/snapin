{
  "name": "snapin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:production": "npm run build",
    "start": "next start",
    "lint": "eslint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "icons": "npx @svgr/cli --out-dir src/shared/ui/icons --template src/shared/lib/svgr-component-template.js --index-template src/shared/lib/svgr-index-template.js --typescript --jsx-runtime automatic --replace-attr-values #fff=currentColor --replace-attr-values #000=currentColor --icon -- src/shared/ui/assets/svg",
    "prettier": "npx prettier . --write",
    "format:fix": "npm run prettier && npm run lint -- --fix"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@tanstack/react-query": "^5.90.5",
    "clsx": "^2.1.1",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@storybook/nextjs": "^9.1.13",
    "@svgr/cli": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.13",
    "prettier": "^3.6.2",
    "storybook": "^9.1.13",
    "typescript": "^5"
  }
}
